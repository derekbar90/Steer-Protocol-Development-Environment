{
  "name": "@steerprotocol/strategy-template-assemblyscript",
  "version": "0.0.1",
  "description": "AssemblyScript based strategy template for the Steer Protocol",
  "main": "index.js",
  "scripts": {
    "test": "yarn jest",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "start": "npx serve .",
    "docs": "typedoc --tsconfig ./tsconfig.json",
    "strategy": "yarn asbuild && yarn test",
    "semantic-release": "semantic-release",
    "commit": "cz",
    "postinstall": "yarn asbuild",
    "backtest-local": "yarn steer-toolset app backtest ./build/debug.wasm -i",
    "steer-toolset": "steer-toolset",
    "backtest": "yarn steer-toolset app backtest -i"
  },
  "author": "Derek Barrera <derekbarrera@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@assemblyscript/loader": "0.19.9",
    "@babel/preset-typescript": "^7.14.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@steerprotocol/app-loader": "^0.1.1",
    "@steerprotocol/base-strategy": "^0.1.0",
    "@steerprotocol/concentrated-liquidity-strategy": "^0.2.1",
    "@steerprotocol/strategy-utils": "^2.1.7",
    "as-bignum": "^0.2.23",
    "as-bind": "^0.8.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "^4.6.0",
    "json-as": "^0.5.23",
    "typescript": "^4.3.5",
    "visitor-as": "^0.11.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.8",
    "@steerprotocol/steer-toolset": "^1.4.2",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.11.11",
    "assemblyscript": "^0.27.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.0.6",
    "cz-conventional-changelog": "3.3.0",
    "jest": "^27.0.6",
    "regenerator-runtime": "^0.13.9",
    "semantic-release": "^17.4.5",
    "typedoc": "^0.21.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
